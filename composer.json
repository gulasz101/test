{
    "name": "pingencom/pingenv2",
    "type": "library",
    "description": "php integration for pingen.",
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "league/oauth2-client": "^2.6",
        "illuminate/support": "^8.17",
        "illuminate/http": "^8.17",
        "vlucas/phpdotenv": "^5.2",
        "spatie/data-transfer-object": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "Pingen\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Lourens Systems GmbH",
            "email": "info@lourenssystems.ch"
        }
    ],
    "require-dev": {
        "php-parallel-lint/php-parallel-lint": "^1.2",
        "symplify/easy-coding-standard-prefixed": "^9.0",
        "phpstan/phpstan": "0.12.42",
        "nikic/php-parser": "4.9.*",
        "phpunit/phpunit": "^9.3",
        "rector/rector": "0.8.7"
    }
}
